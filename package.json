{
  "name": "challenge-match-transactions-api",
  "version": "1.0.0",
  "description": "This is a four-part challenge designed to assess your ability to write code that matches transaction records to order records, while considering various complications and deployment steps",
  "main": "server.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "npm install && npx ts-node-transpile-only src/server.ts",
    "start:prod": "npm run build && node build/server.js",
    "start:dev": "nodemon",
    "lint": "eslint . ",
    "test": "jest",
    "precommit": "lint-staged",
    "prepare": "sh ./scripts/prepare.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "keywords": [],
  "author": "Matheus Icaro",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "matheusicaro-node-framework": "^1.1.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@faker-js/faker": "^9.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.8.5",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fishery": "^2.2.2",
    "globals": "^15.12.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "run-script-os": "^1.1.6",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  }
}
